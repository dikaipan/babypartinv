{
  "expo": {
    "name": "BabyPart Inventory",
    "slug": "babypart-inventory",
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "scheme": "babypart",
    "newArchEnabled": true,
    "splash": {
      "backgroundColor": "#0A0F1A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.babypart.inventory"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0A0F1A"
      },
      "package": "com.babypart.inventory",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "onesignal-expo-plugin",
        {
          "mode": "development"
        }
      ]
    ]
  }
}
